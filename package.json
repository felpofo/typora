{
  "name": "catppuccin-typora",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "yarn clean && sass --no-source-map src/styles/:dist/ && yarn post-build",
    "watch": "sass --no-source-map src/styles/:dist/ --watch",
    "dev": "yarn clean && yarn build && yarn post-build",
    "copy-fonts": "cp src/fonts/*.ttf dist/catppuccin",
    "post-build": "mkdir -p dist/catppuccin && yarn copy-fonts",
    "clean": "rm -rf dist"
  },
  "repository": "git@github.com:felpofo/typora",
  "author": "felpofo <felipepitolpuhl@gmail.com>",
  "dependencies": {},
  "devDependencies": {
    "sass": "^1.52.3"
  }
}
